{
  "id": "/workloads/jenkins",
  "role": "workloads",
  "cmd": null,
  "cpus": 1,
  "mem": 4096,
  "disk": 0,
  "gpus": 0,
  "instances": 1,
  "acceptedResourceRoles": [],
  "container": {
    "type": "DOCKER",
    "docker": {
      "forcePullImage": false,
      "image": "mesosphere/jenkins:3.6.1-2.190.1",
      "parameters": [],
      "privileged": false
    },
    "volumes": [
      {
        "containerPath": "jenkins_home",
        "persistent": {
          "type": "root",
          "size": 2048,
          "constraints": []
        },
        "mode": "RW"
      },
      {
        "containerPath": "/var/jenkins_home",
        "hostPath": "jenkins_home",
        "mode": "RW"
      }
    ]
  },
  "env": {
    "PROMETHEUS_ENDPOINT": "v1/metrics/prometheus",
    "SSH_KNOWN_HOSTS": "github.com",
    "JENKINS_CONTEXT": "/service/workloads/jenkins",
    "JENKINS_AGENT_ROLE": "*",
    "JVM_OPTS": "-Xms1024m -Xmx1024m",
    "JENKINS_HOME": "/var/jenkins_home",
    "JENKINS_MESOS_MASTER": "zk://zk-1.zk:2181,zk-2.zk:2181,zk-3.zk:2181,zk-4.zk:2181,zk-5.zk:2181/mesos",
    "JENKINS_AGENT_USER": "root",
    "JENKINS_OPTS": "",
    "JENKINS_FRAMEWORK_NAME": "workloads/jenkins",
    "MARATHON_NAME": "marathon"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 30,
      "intervalSeconds": 60,
      "maxConsecutiveFailures": 3,
      "path": "/service/workloads/jenkins",
      "portIndex": 0,
      "protocol": "MESOS_HTTP",
      "ipProtocol": "IPv4",
      "timeoutSeconds": 20,
      "delaySeconds": 15
    }
  ],
  "labels": {
    "DCOS_PACKAGE_SOURCE": "https://universe.mesosphere.com/repo",
    "DCOS_SERVICE_NAME": "workloads/jenkins",
    "DCOS_PACKAGE_FRAMEWORK_NAME": "workloads/jenkins",
    "DCOS_PACKAGE_VERSION": "3.6.1-2.190.1",
    "DCOS_PACKAGE_OPTIONS": "",
    "DCOS_SERVICE_SCHEME": "http",
    "DCOS_SERVICE_PORT_INDEX": "0",
    "DCOS_PACKAGE_DEFINITION": "",
    "DCOS_PACKAGE_NAME": "jenkins",
    "MARATHON_SINGLE_INSTANCE_APP": "true"
  },
  "portDefinitions": [
    {
      "port": 10010,
      "name": "nginx",
      "protocol": "tcp"
    },
    {
      "port": 10011,
      "name": "jenkins",
      "protocol": "tcp"
    },
    {
      "port": 10012,
      "name": "agent",
      "protocol": "tcp"
    }
  ],
  "maxLaunchDelaySeconds": 300,
  "requirePorts": true,
  "upgradeStrategy": {
    "maximumOverCapacity": 0,
    "minimumHealthCapacity": 0
  }
}
